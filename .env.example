# Snowflake Connection Configuration
# Copy this file to .env and fill in your actual credentials
# DO NOT commit .env to version control!

# Snowflake Account (from your URL: https://app.snowflake.com/{account}/...)
# Example: gcmbyoq-ehb83410
SNOWFLAKE_ACCOUNT=your_account_here

# Snowflake Credentials
SNOWFLAKE_USER=your_username_here
SNOWFLAKE_PASSWORD=your_password_here

# Snowflake Resources
SNOWFLAKE_WAREHOUSE=your_warehouse_here
SNOWFLAKE_DATABASE=your_database_here
SNOWFLAKE_SCHEMA=your_schema_here

# Optional: Role (if not using default)
# SNOWFLAKE_ROLE=your_role_here
